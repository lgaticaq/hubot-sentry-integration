{
  "name": "hubot-sentry-integration",
  "version": "2.0.2",
  "description": "Clone sentry integration with slack",
  "main": "src",
  "scripts": {
    "lint": "eslint --fix '{src,test}/**/*.js'",
    "format": "prettier-standard '{src,test}/**/*.js'",
    "ts-compile-check": "tsc -p tsconfig.json --noEmit",
    "test": "nyc mocha"
  },
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lgaticaq/hubot-sentry-integration.git"
  },
  "keywords": [
    "hubot",
    "hubot-scripts",
    "sentry",
    "slack"
  ],
  "author": "Leonardo Gatica <lgatica@protonmail.com> (https://about.me/lgatica)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lgaticaq/hubot-sentry-integration/issues"
  },
  "homepage": "https://github.com/lgaticaq/hubot-sentry-integration#readme",
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@eclass/eslint-config": "2.0.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.0",
    "@semantic-release/npm": "7.0.10",
    "@semantic-release/release-notes-generator": "9.0.1",
    "@slack/client": "3.16.1-sec.2",
    "@types/hubot": "3.3.0",
    "@types/mocha": "8.2.0",
    "@types/sinon": "9.0.10",
    "@types/sinon-chai": "3.2.5",
    "chai": "4.3.0",
    "eslint": "7.19.0",
    "eslint-config-jsdoc-strict": "2.0.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-array-func": "3.1.7",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.13",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.3.1",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-sonarjs": "0.5.0",
    "eslint-plugin-standard": "4.1.0",
    "hubot-slack": "4.9.0",
    "hubot-test-helper": "1.9.0",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "mocha": "8.3.0",
    "nyc": "15.1.0",
    "nyc-config-common": "1.0.1",
    "prettier-standard": "16.4.1",
    "semantic-release": "17.3.8",
    "sinon": "9.2.4",
    "sinon-chai": "3.5.0",
    "typescript": "4.1.5"
  },
  "peerDependencies": {
    "hubot-slack": "^4.8.1"
  },
  "eslintConfig": {
    "extends": [
      "@eclass"
    ]
  },
  "eslintIgnore": [
    "coverage"
  ],
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  },
  "mocha": {
    "exit": true,
    "no-timeouts": true
  },
  "nyc": {
    "extends": "nyc-config-common"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.js": [
      "prettier-standard --lint"
    ],
    "{src,test}/**/*.ts": [
      "prettier-standard"
    ]
  },
  "renovate": {
    "automerge": "minor",
    "extends": [
      "config:js-lib"
    ]
  }
}
